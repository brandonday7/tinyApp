<!DOCTYPE html>
<head>
  <title>TinyApp</title>
  <% include ./partials/_header.ejs %>
</head>
<body>
  <h1>
    <% for (key in urls) { %>
      <li><%= key %> : <%= urls[key]; %></li>
      <form method="POST" action="/urls/<%= key %>/delete">
        <a href="http://localhost:8080/urls/<%= key %>">Edit</a>
        <button>Delete</button>
      </form>
    <% } %>
  </h1>
  <a href="deadLink">Shorten URL</a>
</body>